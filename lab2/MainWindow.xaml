<Window x:Class="lab2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="813">
    <Grid Width="815" >
        <Canvas Background="Cyan" >
            <Label Content="Label" FontSize="14" Canvas.Left="20" Canvas.Top="20"/>
            <TextBox TextWrapping="Wrap" Text="TextBox" Canvas.Top="40" Width="66" FontSize="14" Canvas.Left="40" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Button Content="Button" Canvas.Top="60" FontSize="14" Canvas.Left="60"/>
        </Canvas>
    </Grid>
</Window>
